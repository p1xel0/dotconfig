#!/bin/bash
state="$(pactl list cards | grep -A25 'Name: alsa_card.usb-JDS_Labs_JDS_Labs_Element_II-00' | awk '/Active Profile: /{ print $3 }')"
if [ "$state" = 'output:analog-stereo' ]; then
	pactl set-card-profile alsa_card.usb-JDS_Labs_JDS_Labs_Element_II-00 off
else
	pactl set-card-profile alsa_card.usb-JDS_Labs_JDS_Labs_Element_II-00 output:analog-stereo
fi
